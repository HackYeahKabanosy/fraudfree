{
    "manifest_version": 3,
    "name": "FraudFree - Domain Safety Analyzer",
    "version": "1.0",
    "description": "Analyzes domain safety and scam probability.",
    "permissions": [
      "tabs",
      "activeTab"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "icons": {
      "128": "icon.png"
    }
  }
  